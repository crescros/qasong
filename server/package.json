{
  "name": "artistify-server",
  "version": "0.5.0",
  "description": "serve artistify-frontend to clients, and provide video search functionality from youtube data api",
  "homepage": "https://github.com/callbacc/Artistify",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "cd frontend && npm run build",
    "test": "npx cypress run",
    "dev": "nodemon server/index.js",
    "cy:open": "npx cypress open",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:callbacc/Artistify.git"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cypress-dotenv": "^1.2.2",
    "eslint": "^7.10.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "axios": "^0.20.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "immutability-helper": "^3.1.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "react-uuid": "^1.0.2",
    "rootpath": "^0.1.2",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "yt-search": "^2.3.2"
  }
}
